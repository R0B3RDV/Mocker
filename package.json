{
  "name": "mocker",
  "productName": "Mocker",
  "version": "3.1.1",
  "description": "HTTP/HTTPS mock tool, based on proxy server.",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "package": "npm-run-all package:*",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=static/image/mocker.ico",
    "package:installer": "node ./installer.js",
    "package:inno": "cd inno && ISCC.exe ../setup.iss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eshengsky/Mocker.git"
  },
  "keywords": [
    "mocker",
    "mock",
    "mock-server",
    "mock-data",
    "electron",
    "electron-app"
  ],
  "author": "Sky Sun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eshengsky/Mocker/issues"
  },
  "homepage": "https://github.com/eshengsky/Mocker#readme",
  "devDependencies": {
    "electron": "^2.0.2",
    "electron-winstaller": "^2.6.3",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "font-awesome": "^4.7.0",
    "fuelux": "^3.16.2",
    "http-mitm-proxy": "^0.6.0",
    "i18n": "^0.8.3",
    "jquery": "^3.2.1",
    "log4js": "^2.4.1",
    "monaco-editor": "^0.10.1",
    "request": "^2.83.0",
    "shortid": "^2.2.8",
    "showdown": "^1.8.6",
    "sweetalert2": "^7.1.1",
    "switchery": "git+https://github.com/abpetkov/switchery.git"
  }
}
